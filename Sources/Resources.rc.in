#include <windows.h>

LANGUAGE LANG_NEUTRAL, SUBLANG_DEFAULT

VS_VERSION_INFO VERSIONINFO
 FILEVERSION @NCINE_VERSION_MAJOR@, @NCINE_VERSION_MINOR@, @PACKAGE_VERSION_PATCH@, @PACKAGE_VERSION_REV@
 PRODUCTVERSION @NCINE_VERSION_MAJOR@, @NCINE_VERSION_MINOR@, @PACKAGE_VERSION_PATCH@, @PACKAGE_VERSION_REV@
 FILEFLAGSMASK VS_FFI_FILEFLAGSMASK
#ifdef DEATH_DEBUG
 FILEFLAGS VS_FF_DEBUG
#else
 FILEFLAGS 0x0L
#endif
 FILEOS VOS_NT_WINDOWS32
 FILETYPE VFT_APP
 FILESUBTYPE VFT2_UNKNOWN
{
	BLOCK "StringFileInfo"
	{
		BLOCK "040904b0"
		{
			VALUE "CompanyName", "@NCINE_APP_VENDOR@"
			VALUE "FileDescription", "@NCINE_APP_NAME@: @NCINE_APP_DESCRIPTION@"
			VALUE "FileVersion", "@NCINE_VERSION_MAJOR@.@NCINE_VERSION_MINOR@.@PACKAGE_VERSION_PATCH@.@PACKAGE_VERSION_REV@"
			VALUE "InternalName", "@PACKAGE_EXECUTABLE_NAME@"
			VALUE "LegalCopyright", "© 2016-@NCINE_BUILD_YEAR@ @NCINE_APP_VENDOR@"
#ifdef DEATH_DEBUG
			VALUE "OriginalFilename", "@PACKAGE_EXECUTABLE_NAME@@DEATH_DEBUG_POSTFIX@@PACKAGE_EXTENSION@"
#else
			VALUE "OriginalFilename", "@PACKAGE_EXECUTABLE_NAME@@PACKAGE_EXTENSION@"
#endif
			VALUE "ProductName", "@NCINE_APP_NAME@"
			VALUE "ProductVersion", "@NCINE_VERSION@"
		}
	}
	BLOCK "VarFileInfo"
	{
		VALUE "Translation", 0x0, 1200
	}
}
